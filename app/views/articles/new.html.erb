<h1>New Article</h1>

<%= form_for :article, url: articles_path do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field:title %>
  </p>
     <%= f.label :author %><br>
    
    <%= f.text_field:authoer %>
  </p>
   <p>
    <%= f.label :email %><br>
    
    <%= f.text_field:email %>
  </p>
   <p>
    <%= f.label :phone_number %><br>
    
    <%= f.text_field:phone_number %>
  </p>
 
  <p>
    <%= f.label :text %><br>

    <%= f.text_area:text %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>

<%= link_to 'Back', articles_path %>

<% end %>